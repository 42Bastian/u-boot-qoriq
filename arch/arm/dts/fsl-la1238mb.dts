/dts-v1/;

#include "fsl-lx2160a.dtsi"

/ {
	model = "NXP Layerscape LA1238MB Board";
	compatible = "fsl,la1238mb", "fsl,lx2160a";

	aliases {
		spi0 = &fspi;
	};
};

&fspi {
	status = "okay";

	mt35xu512aba0: flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		spi-max-frequency = <50000000>;
		reg = <0>;
		spi-rx-bus-width = <8>;
		spi-tx-bus-width = <1>;
	};
};

&usb0 {
	status = "disabled";
};

&usb1 {
	status = "disabled";
};

&esdhc1 {
	status = "okay";
	mmc-hs200-1_8v;
	mmc-hs400-1_8v;
	bus-width = <8>;
};

&i2c0 {
	status = "okay";
	u-boot,dm-pre-reloc;
};

&i2c1 {
	status = "okay";
};

&i2c2 {
	status = "okay";
};

&pcie1 {
	status = "disabled";
};

&pcie2 {
	status = "disabled";
};

&pcie5 {
	status = "disabled";
};

&pcie6 {
	status = "disabled";
};
