/dts-v1/;
/include/ "fsl-ls1046a.dtsi"

/ {
	model = "LS1046A TP1 Board";

	aliases {
		spi0 = &qspi;
	};

};

&qspi {
	bus-num = <0>;
	status = "okay";

	qflash0: mt25qu512abb8esf@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spi-flash";
		spi-max-frequency = <50000000>;
		reg = <0>;
	};

};

&esdhc {
	non-removable;
};


&usb0 {
	status = "okay";
};

&usb1 {
	status = "okay";
};

&usb2 {
        status = "okay";
};

